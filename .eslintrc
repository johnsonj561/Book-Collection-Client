{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-shadow": 0,
    "consistent-return": 0,
    "react/forbid-prop-types": 0,
    "react/destructuring-assignment": 0,
    "react/no-unescaped-entities": 0
  }
}